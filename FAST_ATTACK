game.StarterGui:SetCore("SendNotification", {
    Title = "‚ö°FAST ATTACKüëä";
    Text = "Fast attack will automatically turn on when you play Blox Fruit.";
    Duration = 5;
    Icon = "rbxthumb://type=Asset&id=91623223092520&w=150&h=150";
})

while true do
    wait(0.001) -- L·∫∑p l·∫°i m·ªói 0.01s

    -- L·∫•y t·∫•t c·∫£ ng∆∞·ªùi ch∆°i trong game
    for _, player in pairs(game:GetService("Players"):GetPlayers()) do
        -- Ki·ªÉm tra xem nh√¢n v·∫≠t c·ªßa ng∆∞·ªùi ch∆°i c√≥ t·ªìn t·∫°i kh√¥ng
        if player.Character and player.Character:FindFirstChild("Head") then
            local args = {
                [1] = player.Character.Head,  -- L·∫•y ph·∫ßn ƒë·∫ßu c·ªßa ng∆∞·ªùi ch∆°i
                [2] = {},
                [4] = "326880d6"
            }

            -- G·ªçi s·ª± ki·ªán FireServer ƒë·ªÉ g·ª≠i y√™u c·∫ßu
            game:GetService("ReplicatedStorage"):WaitForChild("Modules"):WaitForChild("Net"):WaitForChild("RE/RegisterHit"):FireServer(unpack(args))
        end
    end
end
